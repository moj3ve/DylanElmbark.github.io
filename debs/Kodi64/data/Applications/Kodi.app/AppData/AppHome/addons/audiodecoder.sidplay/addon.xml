<?xml version="1.0" encoding="UTF-8"?>
<addon
  id="audiodecoder.sidplay"
  version="1.2.1"
  name="Sidplay Audio Decoder"
  provider-name="spiff">
  <requires>
<import addon="kodi.binary.global.main" version="1.0.14"/>
<import addon="kodi.binary.global.filesystem" version="1.0.2"/>
<import addon="kodi.binary.instance.audiodecoder" version="2.0.0"/></requires>
  <extension
    point="kodi.audiodecoder"
    name="sid"
    extension=".sid|.sidstream"
    tags="true"
    tracks="true"
    library_ios="audiodecoder.sidplay.1.2.1.dylib"/>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">SID (MOS 6581) Audio Decoder</summary>
    <description lang="en">Sound Interface Device (SID) is a format that houses Commodore 64 and 128 music.

This is a addon that implements the emulation of the C64's SID chip (MOS 6581) and CPU (6510) by .SID or .SIDSTREAM files.</description>
    <platform>ios-aarch64</platform>
  </extension>
</addon>
